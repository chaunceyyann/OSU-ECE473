# some more ls aliases
alias ll='ls -lhF --color=auto --group-directories-first'
alias l='ls -ChF --color=auto --group-directories-first'
alias lsa='ls -Alh --color=auto --group-directories-first'
alias x='exit'
alias ..='cd ..'
alias mkdir='mkdir -p'
alias h='history'
alias du='du -hk'
alias df='df -hTk'

 
